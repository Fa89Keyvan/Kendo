<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Kendo.NetCore</title>

    <link href="~/kendo/css/web/kendo.common.min.css" rel="stylesheet" />
    <link href="~/kendo/css/web/kendo.material.min.css" rel="stylesheet" />
    <script src="~/kendo/js/jquery.min.js"></script>
    <script src="~/kendo/js/kendo.web.min.js"></script>
    <script src="~/kendo/js/kendo.aspnetmvc.min.js"></script>
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2018 - Kendo.NetCore</p>
        </footer>
    </div>

    

    @RenderSection("Scripts", required: false)
</body>
</html>
